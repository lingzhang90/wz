<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	
    <!-- 这是没有登陆用户时的背景按钮 -->
    <!-- pressed -->
    <item android:drawable="@drawable/btn_login_pressed" android:state_enabled="true" android:state_pressed="true"/>
    <!-- focused -->
    <item android:drawable="@drawable/btn_login_pressed" android:state_enabled="true" android:state_focused="true"/>
    <!-- normal -->
    <item android:drawable="@drawable/btn_login_normal" android:state_enabled="true"/>

</selector>